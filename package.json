{
    "name": "fish-ide",
    "displayName": "fish-ide",
    "description": "IDE for fish scripts",
    "version": "0.1.0",
    "homepage": "https://marketplace.visualstudio.com/items?itemName=lunaryorn.fish-ide",
    "bugs": "https://github.com/lunaryorn/vscode-fish-ide/issues",
    "author": {
        "name": "Sebastian Wiesner",
        "email": "swiesner@lunaryorn.com",
        "url": "http://www.lunaryorn.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lunaryorn/vscode-fish-ide.git"
    },
    "license": "GPL-3.0",
    "publisher": "lunaryorn",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages",
        "Linters",
        "Formatters"
    ],
    "keywords": [
        "fish",
        "shell",
        "fish_indent",
        "IDE"
    ],
    "badges": [
        {
            "url": "https://travis-ci.org/lunaryorn/vscode-fish-ide.svg?branch=master",
            "href": "https://travis-ci.org/lunaryorn/vscode-fish-ide",
            "description": "Build Status"
        }s
    ],
    "activationEvents": [
        "onLanguage:fish"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "fish",
                "aliases": [
                    "fish"
                ],
                "extensions": [
                    ".fish"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/node": "^6.0.40",
        "tslint": "^5.2.0",
        "tslint-language-service": "^0.9.6",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    }
}
